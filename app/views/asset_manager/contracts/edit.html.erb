<p> Edit Contract </p>
<% form_for [:asset_manager, @contract] do |f| -%>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :contract %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.submit "Edit" %>
  </p>
<% end -%>
<%= link_to "Back", :back , :class => "lightBlue" %>
